<?xml version="1.0" encoding="UTF-8"?>
<!--po设计模式
	1、将页面元素抽出来定义在xml文件当中
	2、基于页面去封装当前页面的元素（面向页面，一个页面就是一个对象）
	3、通过dom4j解析出来元素的数据
	4、通过解析出的元素数据调用相应的元素定位方法（by.id（）,by.name（）等）
-->
<Pages>
    <!-- Page定义的是一个web页面 -->
    <!-- keyword属性：页面的关键字信息 -->
    <Page keyword="百度页">
        <!-- keyword属性：页面元素的关键字信息 -->
        <!-- by属性：告诉调用者在处理这个UIElement页面元素时候根据什么方式去定位元素 -->
        <!-- value属性：by属性需要传入的值 -->
        <UIElement keyword="百度搜索框" by="id" value="kw"></UIElement>
        <UIElement keyword="百度一下" by="id" value="su"></UIElement>
    </Page>

</Pages>